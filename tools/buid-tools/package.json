{
  "name": "@tikkhun/build",
  "version": "0.0.5",
  "description": "",
  "bin": {
    "tikkhun-build": "dist/cli/main.js"
  },
  "main": "dist/lib/index.js",
  "module": "dist/lib/index.mjs",
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/lib/index.d.mts",
        "default": "./dist/lib/index.mjs"
      },
      "require": {
        "default": "./dist/lib/index.js",
        "types": "./dist/lib/index.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx ./src/index.ts",
    "dev:cli": "tsx ./cli/main.ts",
    "test": "vitest",
    "deps:update": "ncu -u"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "build",
    "pack",
    "dist",
    "obfuscator"
  ],
  "author": "tikkhun",
  "license": "MIT",
  "homepage": "https://github.com/radiorz/project-base/tree/main/tools/build-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/radiorz/project-base"
  },
  "dependencies": {
    "@tikkhun/utils": "workspace:^",
    "@tikkhun/cli-core": "workspace:^",
    "@tikkhun/logger": "workspace:^",
    "@tikkhun/utils-core": "workspace:^",
    "glob": "^11.0.0",
    "javascript-obfuscator": "^4.1.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.7.9",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=14"
  }
}
