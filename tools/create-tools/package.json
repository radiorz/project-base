{
  "name": "@tikkhun/create",
  "version": "0.2.8",
  "description": "创建项目",
  "main": "dist/lib/index.js",
  "module": "dist/lib/index.mjs",
  "types": "dist/lib/index.d.ts",
  "bin": {
    "tikkhun-create": "./dist/cli/main.js",
    "tikkhun-create-manage": "./dist/cli/creator-manager.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/lib/index.d.mts",
        "default": "./dist/lib/index.mjs"
      },
      "require": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/lib/index.js"
      }
    }
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/lib/index.js",
    "module": "dist/lib/index.mjs",
    "types": "dist/lib/index.d.ts",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/lib/index.d.mts",
          "default": "./dist/lib/index.mjs"
        },
        "require": {
          "default": "./dist/lib/index.js",
          "types": "./dist/lib/index.d.ts"
        }
      }
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx ./cli/main.ts",
    "test": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.0.10",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.6.0",
    "@tikkhun/cli-core": "workspace:^",
    "@tikkhun/logger": "workspace:^",
    "@tikkhun/utils-core": "workspace:^",
    "commander": "^14.0.0",
    "ejs": "^3.1.10",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.3",
    "lodash": "^4.17.21",
    "minimatch": "^10.0.3"
  }
